import java.util.*; 

public class Solution4 {

	public static void main(String[] args) {
		Scanner jumlahBarang = new Scanner(System.in);	
		int n=jumlahBarang.nextInt();
		int gajiPokok=500000;
		int gajiTotal1=(gajiPokok)-((gajiPokok*15)/100);
		int gajiTotal2=(gajiPokok)+((gajiPokok*25)/100);
		int gajiTotal3=(gajiPokok)+((gajiPokok*35)/100);
		
		if(n <= 15) {
			System.out.println(gajiPokok1);
		}
		if(n >= 40 && n <80) {
			System.out.println(gajiPokok2);
		}
		if(n >= 80) {
			System.out.println(gajiPokok3);
		}
	}

}
