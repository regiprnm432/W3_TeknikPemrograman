 */

import java.math.*;
import java.util.Scanner;	

public class Solution6 {
   public static void main(String []args){
       Scanner input = new Scanner(System.in);
       String a, b;
       a = input.nextLine();
       b =input.nextLine();
       input.close();	//Digunakan untuk menutup koneksi input stream
       
       System.out.println(new BigInteger(a).add(new BigInteger(b)));	//Menampilkan penjumlahan dua bilangan
       System.out.println(new BigInteger(a).multiply(new BigInteger(b)));	//Menampilkan perkalian dua bilangan
   }
}