import java.util.Scanner;

public class Solution3 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		String operasi =  input.nextLine().replaceAll("\\s", "");
		input.close();
		System.out.println(operasi);
	}
}
