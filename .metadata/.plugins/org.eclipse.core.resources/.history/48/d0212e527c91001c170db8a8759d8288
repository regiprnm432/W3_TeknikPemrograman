import java.util.*; 
import java.math.*;

public class Solution4 {

	public static void main(String[] args) {
		Scanner jumlahBarang = new Scanner(System.in);	
		int n=jumlahBarang.nextInt();
		int gajiPokok=500000;
		 System.out.println(new BigInteger(gajiPokok).multiply(new BigInteger(n)));
		
	}

}
